{
  "name": "dbdump",
  "version": "1.1.0",
  "description": "A powerful PostgreSQL schema comparison tool with advanced migration features",
  "main": "src/api.ts",
  "type": "module",
  "exports": {
    ".": "./src/api.ts",
    "./cli": "./compareordumpdbs.ts"
  },
  "scripts": {
    "compare": "bun run compareordumpdbs.ts",
    "help": "bun run compareordumpdbs.ts --help",
    "build": "bun build --compile --outfile dbdump ./compareordumpdbs.ts",
    "install-global": "bun run build && sudo cp dbdump /usr/local/bin/dbdump && echo 'Installed dbdump to /usr/local/bin/dbdump'"
  },
  "keywords": [
    "postgresql",
    "postgres",
    "database",
    "schema",
    "migration",
    "comparison",
    "diff",
    "bun"
  ],
  "author": "Khalil Selyan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KhalilSelyan/dbdump"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.15.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "pg": "^8.16.3",
    "picocolors": "^1.1.1"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
